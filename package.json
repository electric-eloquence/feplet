{
  "name": "feplet",
  "version": "0.2.0",
  "description": "A Mustache-compatible templating engine. Powerful under the hood. Simple behind the wheel.",
  "main": "src/index.js",
  "scripts": {
    "bundle": "babel src -o dist/feplet.node.es5.js && node run/bundle-client.js",
    "lint": "eslint run && eslint src && eslint test",
    "serve": "http-server",
    "start": "npm run watch & npm run serve",
    "test": "mocha test/tests.js",
    "watch": "nodemon --watch src -e js --exec 'npm run bundle'"
  },
  "husky": {
    "hooks": {
      "post-commit": "npm run bundle",
      "pre-push": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/feplet.git"
  },
  "keywords": [
    "fepper",
    "functional",
    "mustache",
    "template",
    "templating"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/feplet/issues"
  },
  "homepage": "https://github.com/electric-eloquence/feplet#readme",
  "dependencies": {
    "json-eval": "^1.0.5",
    "mkdirp": "0.5.1",
    "nopt": "3.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^14.4.0",
    "chai": "4.x",
    "eslint": "3.x",
    "http-server": "^0.11.1",
    "husky": "^1.2.0",
    "mocha": "5.x",
    "nodemon": "^1.14.12",
    "uglify-es": "3.3.9"
  },
  "bin": {
    "hulk": "bin/hulk"
  }
}
